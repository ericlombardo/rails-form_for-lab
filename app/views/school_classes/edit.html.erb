<h1>Edit Class Form</h1>

<%= form_for(@class) do |f| %>  <!--Use instance to iterate through from and fill in values-->
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :room_number %>
  <%= f.text_field :room_number %><br>

  <%= f.submit %> <!--form_for knows it is updating and puts auto generates value-->
<% end %>
